{
  "name": "ioq3-rcon",
  "version": "1.0.0",
  "description": "Client for Urban Terror RCON protocol with parsing.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "commonjs",
  "exports": {
    "require": "./build/index.js",
    "import": "./build/index.mjs"
  },
  "scripts": {
    "build": "npm run build:clean && npm run compile",
    "build:clean": "rm -rf build/*",
    "compile": "tsc",
    "debug": "ts-node-dev --inspect -- src/index.ts",
    "debug:break": "ts-node-dev --inspect-brk -- src/index.ts",
    "test": "jest",
    "ts-node-dev": "ts-node-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zazama/IOQ3Rcon.git"
  },
  "author": "Jan Metzger <jan.metzger@gmx.net> (https://zazama.de)",
  "keywords": [
    "urbanterror",
    "urban terror",
    "ut4",
    "urt",
    "quake3",
    "rcon"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.37",
    "jest": "^27.2.2",
    "ts-jest": "^27.0.5",
    "typescript": "4.2.3"
  }
}
